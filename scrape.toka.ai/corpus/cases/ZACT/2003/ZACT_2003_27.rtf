\par \pard\plain \ltrpar\s20\cf2\qj{\*\hyphen2\hyphlead2\hyphtrail2\hyphmax0}\rtlch\af7\afs24\lang1025\ltrch\dbch\af7\langfe255\hich\f4\fs24\lang7177\loch\f4\fs24\lang7177 

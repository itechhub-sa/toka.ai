\par \pard\plain \ltrpar\s1\cf0\qj\li360\ri0\lin360\rin0\fi0\rtlch\af4\afs24\lang255\ltrch\dbch\af4\langfe255\hich\f4\fs24\lang1033\loch\f4\fs24\lang1033 

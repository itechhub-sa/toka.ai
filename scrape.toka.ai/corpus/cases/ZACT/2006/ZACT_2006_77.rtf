\par \pard\plain \ltrpar\s1\sl360\slmult1\qj{\*\hyphen2\hyphlead2\hyphtrail2\hyphmax0}\li1440\ri0\lin1440\rin0\fi0\rtlch\af10\afs20\lang1025\ltrch\dbch\af10\langfe255\hich\f4\fs16\lang7177\loch\f4\fs16\lang7177 
